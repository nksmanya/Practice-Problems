Link : https://www.geeksforgeeks.org/problems/nth-fibonacci-number1335/1?page=3&sortBy=submissions 
class Solution {
    public int nthFibonacci(int n) {
        // code here
        if(n ==0) return 0;
        if(n ==1) return 1;
        int n1 = 0;
        int n2 = 1;
        int n3 =0;
        for(int i =2; i<=n; i++){
            n3 = n1+n2;
            n1 = n2;
            n2 = n3;
        }
        return n3;
        
    }
}
