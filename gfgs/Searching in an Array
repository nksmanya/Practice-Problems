class Solution {
    public int search(int k, ArrayList<Integer> arr) {
        // code here
        int ans =-1;
        for(int i =0; i<arr.size(); i++){
            if(arr.get(i)==k){
                ans = i+1;
                break;
            }
        }return ans;
    }
}



link : https://www.geeksforgeeks.org/problems/searching-a-number0324/1?page=6&category=Arrays&sortBy=submissions
