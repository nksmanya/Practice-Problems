class Solution {
    public Pair<Integer, Integer> getMinMax(int[] arr) {
        // Code Here
        int max = Integer.MIN_VALUE;
        int min = Integer.MAX_VALUE;
        for(int i : arr){
            if(i < min) min = i;
            if(i > max) max = i;
        }
        return new Pair(min, max);
    }
}
